{
  "apps": [
    {
      "name": "kbj-nestjs-backend",
      "script": "nest start",
      "merge_logs": true,
      "log_date_format": "YYYY/MM/DD HH:mm:ss",
      "env": { "NODE_ENV": "production" },
      "instances": 4,
      "exec_mode": "cluster",
      "node_args": ["--max-http-header-size=16384"]
    }
  ]
}
